<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#040404"
    tools:context="com.carinterface.MainActivity">

    <VideoView
        android:id="@+id/videoView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ToggleButton
        android:id="@+id/toggleButton3"
        android:layout_width="134dp"
        android:layout_height="134dp"
        android:layout_marginStart="225dp"
        android:background="@drawable/checking"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="ToggleButton"
        android:textOff=""
        android:textOn=""
        app:layout_constraintBottom_toBottomOf="@+id/button5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button5" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="119dp"
        android:layout_marginTop="200dp"
        android:ellipsize="marquee"
        android:fontFamily="@font/aclonica"
        android:gravity="center"
        android:marqueeRepeatLimit="marquee_forever"
        android:scrollHorizontally="true"
        android:singleLine="true"
        android:text="No Song Playing"
        android:textColor="#FFFFFF"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/videoView"
        app:layout_constraintVertical_bias="0.656" />

    <Button
        android:id="@+id/button4"
        android:layout_width="119dp"
        android:layout_height="102dp"
        android:layout_marginStart="4dp"
        android:background="@drawable/_36473_200"
        android:contentDescription="@string/app_name"
        android:rotation="-180"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/toggleButton"
        app:layout_constraintEnd_toStartOf="@+id/toggleButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toggleButton" />

    <Button
        android:id="@+id/button"
        android:layout_width="118dp"
        android:layout_height="109dp"
        android:layout_marginTop="700dp"
        android:background="@drawable/_36473_200"
        android:contentDescription="@string/app_name"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/toggleButton"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/toggleButton"
        android:layout_width="128dp"
        android:layout_height="132dp"
        android:background="@drawable/check"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="ToggleButton"
        android:textOff=""
        android:textOn=""
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="132dp"
        android:layout_marginTop="200dp"
        android:fontFamily="@font/aclonica"
        android:gravity="center"
        android:text="No Song Playing"
        android:textColor="#FFFFFF"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/videoView"
        app:layout_constraintVertical_bias="0.577" />

    <Button
        android:id="@+id/button5"
        android:layout_width="165dp"
        android:layout_height="134dp"
        android:layout_marginStart="-130dp"
        android:layout_marginBottom="75dp"
        android:background="@drawable/imageslib"
        android:contentDescription="@string/app_name"
        android:textSize="25sp"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/toggleButton3" />

    <Button
        android:id="@+id/button6"
        android:layout_width="83dp"
        android:layout_height="211dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="1176dp"
        android:background="@android:color/transparent"
        android:fontFamily="sans-serif-thin"
        android:lineSpacingExtra="-120sp"
        android:text=". \n. \n. "
        android:textSize="120sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>